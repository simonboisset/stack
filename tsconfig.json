{
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "module": "commonjs",
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "moduleResolution": "node",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    // "noUnusedLocals": true,
    "skipLibCheck": true,
    "esModuleInterop":true,
    "jsx": "react",
    "incremental": true
  },
  "include": ["src"],
  "exclude": ["**/node_modules", "**/.*/"],
}
